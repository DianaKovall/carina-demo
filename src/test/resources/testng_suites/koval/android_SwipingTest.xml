<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="ANDROID. Carina Demo Application">

    <parameter name="suiteOwner" value="Diana Koval" />
    <parameter name="Environments" value="DEMO" />



    <test name="1. Get Parsed Mail">
        <classes>
            <class name="koval.SwipeCheckTest">
                <methods>
                    <include name="testParseMail"/>
                </methods>
            </class>
        </classes>
    </test>


    <test name="2. Get Topics">
        <classes>
            <class name="koval.SwipeCheckTest">
                <methods>
                    <include name="testGetTopics"/>
                </methods>
            </class>
        </classes>
    </test>

</suite>